<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.habbittracker2.HelloController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" style="-fx-background-color: #4682b4;" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="70.0" layoutY="105.0" prefHeight="21.0" prefWidth="97.0" text="UserName" textFill="#14292c">
               <font>
                  <Font name="Arial" size="18.0" />
               </font></Label>
            <Label layoutX="70.0" layoutY="167.0" prefHeight="40.0" prefWidth="142.0" text="Password" textFill="#14292c">
               <font>
                  <Font name="Arial" size="18.0" />
               </font></Label>
            <PasswordField fx:id="passwordField" layoutX="193.0" layoutY="168.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="39.0" prefWidth="324.0" promptText="at least 4 digits" style="-fx-background-color: #87cefa;">
               <effect>
                  <Blend />
               </effect>
               <font>
                  <Font size="18.0" />
               </font></PasswordField>
            <TextField fx:id="usernameField" layoutX="193.0" layoutY="96.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="39.0" prefWidth="324.0" promptText="ex.Username" style="-fx-background-color: #87cefa;">
               <font>
                  <Font size="18.0" />
               </font>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <effect>
                  <Blend />
               </effect></TextField>
            <Button fx:id="Login" layoutX="219.0" layoutY="249.0" mnemonicParsing="false" onAction="#login" onDragEntered="#ToMain" prefHeight="31.0" prefWidth="179.0" style="-fx-background-color: #87cefa;" text="Let's Build new Habbits" />
            <Label fx:id="L1" layoutX="170.0" layoutY="312.0" prefHeight="40.0" prefWidth="277.0" text="      " textFill="#e41616">
               <font>
                  <Font size="18.0" />
               </font></Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
